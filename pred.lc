zero := (fn f => fn x => x);
succ := fn n => (fn f => fn x => f (n f x));
one := succ zero;
two := succ one;



true := fn t => fn e => t;

false := fn t => fn e => e;

pair := fn f => fn s => fn c => c f s; 

first := fn p => p (true);

second := fn p => p (false);

pred := fn n => ( first (n fn p => pair (second p) (succ (second p)) (pair zero zero)) );


main := pred two;